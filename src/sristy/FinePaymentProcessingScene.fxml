        </columns>
      </TableView>
      <Button layoutX="316.0" layoutY="555.0" mnemonicParsing="false" onAction="#backToDashboardButtonOnClick" text="Go back to Dashboard" />
      <Button layoutX="295.0" layoutY="509.0" mnemonicParsing="false" onAction="#completeSelectedButtonOnClick" text="Mark Selected as Completed" />
   </children>
</AnchorPane>
