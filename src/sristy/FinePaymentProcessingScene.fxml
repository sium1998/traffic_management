            <Font name="Bauhaus 93" size="36.0" />
         </font>
      </Label>
      <TableView fx:id="finePaymentTableView" layoutX="41.0" layoutY="108.0" prefHeight="385.0" prefWidth="719.0">
        <columns>
          <TableColumn fx:id="titleTableColumn" prefWidth="164.0" text="Title" />
